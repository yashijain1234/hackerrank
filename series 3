#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
int a[6];
    int i, s1=0,s2=0,sum=0;
    for(i=0;i<6;i++)
    {
        scanf("%d",&a[i]);
    }
    for(i=0;i<3;i++)
    {
        s1+=a[i];
    }
    for(i=3;i<6;i++)
    {
        s2+=a[i];
    }
     sum=s1*s2;
    printf("%d" , sum);
       
    return 0;
}
